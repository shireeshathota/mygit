#%RAML 1.0 Trait

responses:
  400:
    body:
      application/json:
        example:
          {
            "StatusCode":"400",
            "ErrorMessage": "Bad Request"
          }
  401:
    body:
      application/json:
        example:
          {
            "StatusCode":"401",
            "ErrorMessage": "Un-Authorized Access"
          }          
  404:
    body:
      application/json:
        example:
          {
            "StatusCode":"404",
            "ErrorMessage": "Resource Not Found"
          }

  500:
    body:
      application/json:
        example:
          {
            "StatusCode":"500",
            "ErrorMessage": "Internal Server Error"
          }              